n><span class=3D"code-string">.vb"</span>:
<span class=3D"code-keyword">case</span> <span class=3D"code-string">"</spa=
n><span class=3D"code-string">.nvb"</span>:
    provider =3D <span class=3D"code-keyword">new</span> Microsoft.VisualBa=
sic.VBCodeProvider();
    <span class=3D"code-keyword">break</span>;
<span class=3D"code-keyword">case</span> <span class=3D"code-string">"</spa=
n><span class=3D"code-string">.njs"</span>:
<span class=3D"code-keyword">case</span> <span class=3D"code-string">"</spa=
n><span class=3D"code-string">.js"</span>:
    provider =3D (CodeDomProvider)Activator.CreateInstance(
        <span class=3D"code-string">"</span><span class=3D"code-string">Mic=
rosoft.JScript"</span>,=20
        <span class=3D"code-string">"</span><span class=3D"code-string">Mic=
rosoft.JScript.JScriptCodeProvider"</span>).Unwrap();
    <span class=3D"code-keyword">break</span>;
